{
    "builds": [
      {
        "src": "public/index.php",
        "use": "now-php"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "public/index.php" }
    ],
    "env": {
        "APP_ENV": "local",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourproductionurl.com",


        "CACHE_DRIVER": "file",
        "QUEUE_CONNECTION": "sync"
    }
  }
  