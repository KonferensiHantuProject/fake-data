{
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.5.2" }
    },
    "routes": [{
        "src": "/(.*)",
        "dest": "/api/index.php"
    }],
    "env": {
        "APP_NAME": "Vercel Lumen",
        "APP_ENV": "local",
        "APP_DEBUG": "true",
        "APP_URL": "https://yourproductionurl.com",

        "CACHE_DRIVER": "file",
        "LOG_CHANNEL": "stderr",
        "QUEUE_CONNECTION": "sync",
        "SESSION_DRIVER": "array"
    }
}